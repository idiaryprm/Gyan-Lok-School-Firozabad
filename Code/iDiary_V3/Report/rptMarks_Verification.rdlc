  <Code>Public Function MarksToGrade(ByVal myMarks As String) As String
        'Perform Marks-Grade Mapping for Group-1 (KG-V) Non academic (Languages, etc)
        Dim myEnteredMarks As Double = Val(myMarks.split("/")(0)) / Val(myMarks.split("/")(1)) * 100   'Convert Marks to Scale of 100
        Dim myGrade As String = ""
        If myEnteredMarks &gt; 90 And myEnteredMarks &lt;= 100 Then myGrade = "A1"
        If myEnteredMarks &gt; 80 And myEnteredMarks &lt;= 90 Then myGrade = "A2"
        If myEnteredMarks &gt; 70 And myEnteredMarks &lt;= 80 Then myGrade = "B1"
        If myEnteredMarks &gt; 60 And myEnteredMarks &lt;= 70 Then myGrade = "B2"
        If myEnteredMarks &gt; 50 And myEnteredMarks &lt;= 60 Then myGrade = "C1"
        If myEnteredMarks &gt; 40 And myEnteredMarks &lt;= 50 Then myGrade = "C2"
        If myEnteredMarks &gt; 40 And myEnteredMarks &lt;= 50 Then myGrade = "C2"
        If myEnteredMarks &gt; 32 And myEnteredMarks &lt;= 40 Then myGrade = "D"
        If myEnteredMarks &gt; 20 And myEnteredMarks &lt;= 32 Then myGrade = "E1"
        If myEnteredMarks &lt;= 20 Then myGrade = "E2"


        Return myGrade
    End Function</Code>
