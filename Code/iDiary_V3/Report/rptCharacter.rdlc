  <Code>Public Function DateString(dt As String) As String
            Dim rv As String = ""
            If dt &lt;&gt; "" Then
                Dim a As Date = dt
                rv = a.ToString("dd/MM/yyyy")
            End If
            Return rv
        End Function

Public Function MarksToRank(ByVal myMarks As Double) As String
       
        Dim myEnteredMarks As Double = myMarks
        Dim myGrade As String = ""
        If myEnteredMarks &gt; 60 Then myGrade = "1st"
             If myEnteredMarks &gt; 45 And myEnteredMarks &lt;= 60 Then myGrade = "2nd"
        If myEnteredMarks &gt; 33 And myEnteredMarks &lt;= 45 Then myGrade = "3rd"
        If myEnteredMarks &lt;= 33 Then myGrade = "Failed"


        Return myGrade
    End Function

