  <Code>Dim GroupCounter as Integer=0

Function GetGroupCounter() as String
	GroupCounter=GroupCounter+1
	return GroupCounter.ToString()
End Function

 Public Function getWtMarks(obtM As String, maxM As String, percent As Double, optional decPlace As Integer=1) As String
        Dim rv As String = ""

        If val(maxM) = 0 Then
            rv = ""
        Else
            rv = Math.round(percent * CDbl(obtm) / CDbl(maxm), decPlace, MidpointRounding.AwayFromZero)
        End If
        Return rv
    End Function
 Public Function getMarks(marks As String, index As Integer) As String
        Dim rv As String = ""
        Try
            rv = marks.Split("/")(index)
        Catch ex As Exception
            rv = marks
        End Try
        Return rv
    End Function</Code>
